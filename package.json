{
  "name": "solana-attestation-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docs:dev": "vocs dev",
    "docs:build": "vocs build",
    "docs:preview": "vocs preview"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "srs-lib": "^1.0.0",
    "vocs": "^1.0.11"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.7",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  },
  "prettier": {
    "printWidth": 160,
    "tabWidth": 4,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "quoteProps": "consistent",
